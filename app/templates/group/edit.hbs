<div class="col-md-4 col-sm-8 col-xs-12">
	{{#js-feedback-message isShowed=message.show timer=message.timer type=message.type hasCloseButton=false}}
		{{message.text}}
	{{/js-feedback-message}}
	<label>{{t "application.names.parent"}}:</label>

		{{js-select
			selectedValue=parent
			content=listGroups
			label='path_name'
			prompt="Select group"
			class="form-control"
		}}

		<br/>
	<label>{{t "application.names.name"}}:</label>{{input type="text" value=model.name class="form-control"}}<br />
	<button {{action "save"}} class="btn btn-primary">{{t "application.actions.save"}}</button>
</div>
